[sub_resource type="Shader" id=4]
code = "shader_type canvas_item;

// Vignette
uniform vec4 color : hint_color;
uniform float distanceOffset : hint_range(-2.0, 1.0);
uniform float distanceMultiplier : hint_range(0.0, 4.0);
uniform float noisyness : hint_range(0.0, 10.0);
uniform sampler2D noise;
uniform vec2 CENTER;

void fragment() {
	
	float dist = clamp(distance(UV, CENTER) * 2.0 + distanceOffset, 0.0, 1.0) * distanceMultiplier * (1.0 + texture(noise, UV).r * noisyness);
	
	COLOR = vec4(color.rgb, dist * color.a);
	
}"

